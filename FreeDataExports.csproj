<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Ryan Kueter</Authors>
    <Description>A .NET library available on NuGet that allows you to export your data to a spreadsheet, including .xlsx (2019), .ods (1.3), or CSV files.</Description>
    <Copyright>Ryan Kueter</Copyright>
    <Version>1.0.11</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/ryankueter/FreeDataExports</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
