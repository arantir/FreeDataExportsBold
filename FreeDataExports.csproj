<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.18</Version>
    <Authors>Ryan Kueter</Authors>
    <Company>Ryan Kueter</Company>
    <Description>A .NET library that allows you to export your data to a spreadsheet, including .xlsx (2019), .ods (1.3), and .csv files.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>Icon.jpg</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/ryankueter/FreeDataExports</RepositoryUrl>
    <PackageReleaseNotes>Minor code optimizations (i.e., faster iterators). Performance improvements are really not necessary for this projects, it's already super fast.</PackageReleaseNotes>
    <PackageTags />
    <AssemblyVersion>1.0.18.0</AssemblyVersion>
    <FileVersion>1.0.18.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Artwork\Icon.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
