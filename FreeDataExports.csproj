<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Ryan Kueter</Authors>
    <Description>A .NET library that allows you to export your data to a spreadsheet, including .xlsx (2019), .ods (1.3), and .csv files.</Description>
    <Copyright>Ryan Kueter</Copyright>
    <Version>1.0.13</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/ryankueter/FreeDataExports</RepositoryUrl>
    <PackageIcon>Icon.jpg</PackageIcon>
    <AssemblyVersion>1.0.13.0</AssemblyVersion>
    <FileVersion>1.0.13.0</FileVersion>
    <PackageReleaseNotes>Fixed typo and icon. No code changes.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Artwork\Icon.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
