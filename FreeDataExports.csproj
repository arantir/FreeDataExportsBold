<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.20</Version>
    <Authors>Ryan A. Kueter</Authors>
    <Company>Ryan A. Kueter</Company>
    <Description>A .NET library that allows you to export your data to a spreadsheet, including .xlsx (2019), .ods (1.3), and .csv files.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>Icon.jpg</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/ryankueter/FreeDataExports</RepositoryUrl>
    <PackageReleaseNotes>A NuGet package for .Net Standard 2.0 and .NET 6.</PackageReleaseNotes>
    <PackageTags />
    <AssemblyVersion>1.0.20.0</AssemblyVersion>
    <FileVersion>1.0.20.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Artwork\Icon.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
